{
  "name": "nest-frontend",
  "version": "0.1.0",
  "description": "NestJS frontend application using GOV.UK Frontend",
  "author": "Matt Hobbs",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:govuk": "jest --testPathPattern=govuk-components",
    "test:govuk:watch": "jest --testPathPattern=govuk-components --watch",
    "test:govuk:cov": "jest --testPathPattern=govuk-components --coverage"
  },
  "keywords": [
    "nestjs",
    "govuk",
    "frontend",
    "typescript",
    "nunjucks",
    "express",
    "gov.uk-frontend",
    "uk-government",
    "template"
  ],
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.4",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@types/compression": "^1.7.5",
    "axios": "^1.8.4",
    "cache-manager": "^6.4.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-request-id": "^3.0.0",
    "govuk-frontend": "^5.9.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "nest-winston": "^1.10.2",
    "nunjucks": "^3.2.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/nunjucks": "^3.2.6",
    "@types/serve-static": "^1.15.7",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "node-html-parser": "^6.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.1.3"
  },
  "compilerOptions": {
    "strict": true
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
