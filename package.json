{
  "name": "nest-frontend",
  "version": "0.1.0",
  "description": "NestJS frontend application using GOV.UK Frontend",
  "author": "Matt Hobbs",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/hmcts/NestJS-frontend"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:govuk": "jest --testPathPattern=govuk-components",
    "test:govuk:watch": "jest --testPathPattern=govuk-components --watch",
    "test:govuk:cov": "jest --testPathPattern=govuk-components --coverage",
    "build:frontend": "webpack --config webpack.config.js",
    "build:frontend:watch": "webpack --config webpack.config.js --watch",
    "build:frontend:prod": "NODE_ENV=production webpack --config webpack.config.js"
  },
  "keywords": [
    "nestjs",
    "govuk",
    "frontend",
    "typescript",
    "nunjucks",
    "express",
    "gov.uk-frontend",
    "uk-government",
    "template"
  ],
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.4",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/csurf": "^1.11.5",
    "@types/joi": "^17.2.2",
    "axios": "^1.9.0",
    "cache-manager": "^6.4.2",
    "cache-manager-redis-store": "^3.0.1",
    "chokidar": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-request-id": "^3.0.0",
    "govuk-frontend": "^5.9.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "nest-winston": "^1.10.2",
    "nunjucks": "^3.2.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/nunjucks": "^3.2.6",
    "@types/serve-static": "^1.15.7",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "node-html-parser": "^7.0.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "sass": "^1.71.1",
    "sass-loader": "^16.0.5",
    "supertest": "^7.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.3.2",
    "typescript": "^5.1.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=22.15.0"
  }
}
