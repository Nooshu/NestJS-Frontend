{
  "name": "nest-frontend",
  "version": "0.1.0",
  "description": "NestJS frontend application using GOV.UK Frontend",
  "author": "Matt Hobbs",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/hmcts/NestJS-frontend"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build --config tsconfig.build.json",
    "postbuild": "npm run copy:views && npm run copy:public-assets",
    "build:prod": "npm run build && npm run build:frontend",
    "format": "prettier --write \"src/**/*.{ts,js,json,njk}\"",
    "start": "cross-env NODE_ENV=production PORT=3002 nest start",
    "start:dev": "./filter-concurrently-output.sh npm run build:frontend:dev && concurrently --kill-others-on-fail --handle-input --no-command-logs \"cross-env NODE_ENV=development PORT=3002 nest start --watch\" \"npm run watch:all\"",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:render": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathPatterns=test/unit --coverage",
    "test:integration": "jest --testPathPatterns=test/integration --coverage",
    "test:unit:watch": "jest --testPathPatterns=test/unit --watch",
    "test:integration:watch": "jest --testPathPatterns=test/integration --watch",
    "test:unit:debug": "jest --testPathPatterns=test/unit --runInBand --detectOpenHandles",
    "test:integration:debug": "jest --testPathPatterns=test/integration --runInBand --detectOpenHandles",
    "test:govuk": "jest --testPathPatterns=test/govuk-components --coverage",
    "test:govuk:watch": "jest --testPathPatterns=test/govuk-components --watch",
    "test:govuk:debug": "jest --testPathPatterns=test/govuk-components --runInBand --detectOpenHandles",
    "test:e2e": "playwright test",
    "test:e2e:local": "./scripts/test-e2e.sh",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:browsers": "npm-run-all test:e2e:chromium test:e2e:firefox test:e2e:webkit",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:cov": "jest --coverage --coverageReporters='text-summary' --coverageReporters='html'",
    "test:cov:unit": "jest --testPathPatterns=test/unit --coverage --coverageReporters='text-summary' --coverageReporters='html'",
    "test:cov:integration": "jest --testPathPatterns=test/integration --coverage --coverageReporters='text-summary' --coverageReporters='html'",
    "test:cov:govuk": "jest --testPathPatterns=test/govuk-components --coverage --coverageReporters='text-summary' --coverageReporters='html'",
    "test:all": "npm-run-all test:unit test:integration test:govuk test:e2e",
    "test:all:watch": "concurrently \"npm run test:unit:watch\" \"npm run test:integration:watch\" \"npm run test:govuk:watch\"",
    "build:frontend": "npm run build:scss && npm run copy:assets && npm run copy:views && npm run fingerprint:assets",
    "build:frontend:watch": "./filter-concurrently-output.sh concurrently --kill-others-on-fail --no-command-logs \"npm run watch:all\"",
    "build:scss": "sass src/frontend/scss/main.scss:dist/public/css/main.css --style=compressed --load-path=node_modules",
    "build:scss:watch": "sass src/frontend/scss/main.scss:dist/public/css/main.css --watch --load-path=node_modules",
    "copy:assets": "npm-run-all copy:images copy:js copy:govuk-assets copy:govuk-dist copy:public-assets",
    "copy:images": "cpy 'src/frontend/images/**/*' 'dist/public/images'",
    "copy:js": "cpy 'src/frontend/js/**/*' 'dist/public/js'",
    "copy:govuk-assets": "cpy 'node_modules/govuk-frontend/govuk/assets/**/*' 'dist/public/assets'",
    "copy:govuk-dist": "cpy 'node_modules/govuk-frontend/dist/govuk/*.{css,js,css.map,js.map}' 'dist/public/govuk'",
    "copy:public-assets": "cpy 'src/public/**/*' 'dist/public'",
    "copy:views": "cpy 'src/views/**/*.njk' 'dist/views'",
    "watch:all": "./filter-concurrently-output.sh concurrently --kill-others-on-fail --no-command-logs \"npm run watch:assets\" \"npm run watch:scss\"",
    "watch:assets": "onchange 'src/frontend/images/**/*' 'src/public/**/*' -- npm-run-all copy:images copy:public-assets",
    "watch:scss": "sass src/frontend/scss/main.scss:dist/public/css/main.css --watch --load-path=node_modules",
    "fingerprint:assets": "ts-node -r tsconfig-paths/register src/cli/fingerprint-assets.ts",
    "test:e2e:all": "playwright test",
    "build:frontend:dev": "npm run build:scss && npm run copy:images && npm run copy:js && npm run copy:govuk-assets && npm run copy:govuk-dist",
    "deploy:render": "./scripts/deploy-render.sh"
  },
  "keywords": [
    "nestjs",
    "govuk",
    "frontend",
    "typescript",
    "nunjucks",
    "express",
    "gov.uk-frontend",
    "uk-government",
    "template"
  ],
  "dependencies": {
    "@nestjs/axios": "4.0.1",
    "@nestjs/cache-manager": "3.1.0",
    "@nestjs/common": "11.1.11",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.11",
    "@nestjs/platform-express": "11.1.11",
    "@nestjs/serve-static": "5.0.4",
    "@nestjs/swagger": "11.2.3",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/throttler": "6.5.0",
    "axios": "1.13.2",
    "brace-expansion": "4.0.1",
    "cache-manager": "7.2.7",
    "cache-manager-redis-store": "3.0.1",
    "chokidar": "5.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "csrf-csrf": "4.0.3",
    "express": "5.2.1",
    "express-rate-limit": "8.2.1",
    "express-request-id": "3.0.0",
    "glob": "13.0.0",
    "govuk-frontend": "5.13.0",
    "helmet": "8.1.0",
    "ioredis": "5.9.0",
    "joi": "18.0.2",
    "nest-winston": "1.10.2",
    "nunjucks": "3.2.4",
    "pino": "10.1.0",
    "pino-pretty": "13.1.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "swagger-ui-express": "5.0.1",
    "winston": "3.19.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@nestjs/cli": "11.0.14",
    "@nestjs/testing": "11.1.11",
    "@playwright/test": "1.57.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@types/compression": "1.8.1",
    "@types/cookie-parser": "1.4.10",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.3",
    "@types/nunjucks": "3.2.6",
    "@types/serve-static": "2.2.0",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "autoprefixer": "10.4.23",
    "concurrently": "9.2.1",
    "cpy-cli": "6.0.0",
    "cross-env": "10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jsdom": "27.4.0",
    "node-html-parser": "7.0.1",
    "npm-run-all2": "8.0.4",
    "onchange": "7.1.0",
    "postcss": "8.5.6",
    "prettier": "^3.7.4",
    "rimraf": "6.1.2",
    "sass": "1.97.2",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24.11.1",
    "npm": ">=11.7.0"
  },
  "overrides": {
    "multer": "2.0.2",
    "jest": {
      "engines": {
        "node": ">=18.14.0"
      }
    },
    "nunjucks": {
      "chokidar": "5.0.0"
    }
  }
}
