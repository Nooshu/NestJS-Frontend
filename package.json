{
  "name": "nest-frontend",
  "version": "0.1.0",
  "description": "NestJS frontend application using GOV.UK Frontend",
  "author": "Matt Hobbs",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/hmcts/NestJS-frontend"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:govuk": "jest --testPathPattern=govuk-components",
    "test:govuk:watch": "jest --testPathPattern=govuk-components --watch",
    "test:govuk:cov": "jest --testPathPattern=govuk-components --coverage",
    "build:frontend": "npm run build:scss && npm run copy:assets && npm run fingerprint:assets",
    "build:frontend:watch": "npm run build:scss:watch",
    "build:scss": "sass src/frontend/scss/main.scss:dist/public/css/main.css --style=compressed --load-path=node_modules --update",
    "build:scss:watch": "sass src/frontend/scss/main.scss:dist/public/css/main.css --watch --load-path=node_modules",
    "copy:assets": "cpy 'src/frontend/images/**/*' 'dist/public/images' && cpy 'node_modules/govuk-frontend/govuk/assets/**/*' 'dist/public/assets' && cpy 'node_modules/govuk-frontend/dist/govuk/*.css' 'dist/public/govuk' && cpy 'node_modules/govuk-frontend/dist/govuk/*.css.map' 'dist/public/govuk' && cpy 'node_modules/govuk-frontend/dist/govuk/*.js' 'dist/public/govuk'",
    "fingerprint:assets": "ts-node -r tsconfig-paths/register src/cli/fingerprint-assets.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit"
  },
  "keywords": [
    "nestjs",
    "govuk",
    "frontend",
    "typescript",
    "nunjucks",
    "express",
    "gov.uk-frontend",
    "uk-government",
    "template"
  ],
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.4",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/csurf": "^1.11.5",
    "@types/joi": "^17.2.2",
    "axios": "^1.9.0",
    "cache-manager": "^6.4.2",
    "cache-manager-redis-store": "^3.0.1",
    "chokidar": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-request-id": "^3.0.0",
    "glob": "^11.0.2",
    "govuk-frontend": "^5.10.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "nest-winston": "^1.10.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/nunjucks": "^3.2.6",
    "@types/serve-static": "^1.15.7",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.18",
    "cpy-cli": "^5.0.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "node-html-parser": "^7.0.0",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.35",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "sass": "^1.71.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
